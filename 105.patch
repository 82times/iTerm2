+                    // ANSI
+                case 'Z':
+                    result.type = ANSICSI_CBT;
+                    SET_PARAM_DEFAULT(param,0,1);
+                    break;
+                case 'G':
+                    result.type = ANSICSI_CHA;
+                    SET_PARAM_DEFAULT(param,0,1);
+                    break;
+                case 'd':
+                    result.type = ANSICSI_VPA;
+                    SET_PARAM_DEFAULT(param,0,1);
+                    break;
+                case 'e':
