+#endif
+                        result.type = VT100_NOTSUPPORT;
+                    }
+                        break;
+
+                case 'n':
+                    result.type = VT100CSI_DSR;
+                    SET_PARAM_DEFAULT(param, 0, 0);
+                    break;
+
+                case 'J':
+                    result.type = VT100CSI_ED;
+                    SET_PARAM_DEFAULT(param, 0, 0);
+                    break;
+
+                case 'K':
+                    result.type = VT100CSI_EL;
+                    SET_PARAM_DEFAULT(param, 0, 0);
