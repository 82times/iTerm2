+                            break;
+                        default:
+                            result.type = VT100_NOTSUPPORT;
+                            break;
+                    }
+                    break;
+                case 'S':
+                    result.type = XTERMCC_SU;
+                    SET_PARAM_DEFAULT(param,0,1);
+                    break;
+                case 'T':
+                    if (param.count<2) {
+                        result.type = XTERMCC_SD;
+                        SET_PARAM_DEFAULT(param,0,1);
+                    }
+                    else
+                        result.type = VT100_NOTSUPPORT;
+
+                    break;
+
